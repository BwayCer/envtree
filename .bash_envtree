#!/bin/bash
# 啟用專屬命令行介面


myBash(){
    local dirname_myBash="${HOME}/ys/_userdir/.myBash"
    local target="$dirname_myBash/.myProFile"


    if [ -z "`echo $PATH | grep \":$HOME/ys/capp/bin\"`" ]
    then
        PATH="$PATH:$HOME/ys/capp/bin"
    fi


    while read line
    do
        local fileName=$(echo $line | sed 's/#.*//g')

        if [ "$fileName" ]; then
            source $dirname_myBash/$fileName
        fi
    done < $target
}

myBash

