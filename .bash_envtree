#!/bin/bash
# 啟用專屬命令行介面


myBash(){
    local dirname_myBash="${HOME}/ys/myBash"
    local target="$dirname_myBash/.myProFile"

    while read line
    do
        local fileName=$(echo $line | sed 's/#.*//g')

        if [ "$fileName" ]; then
            source $dirname_myBash/$fileName
        fi
    done < $target
}

myBash

