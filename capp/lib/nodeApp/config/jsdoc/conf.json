{
    "source": {
        "includePattern": ".+\\.js(doc)?$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc",
        "recurse": true,
        "tutorials": "./booklet_scrap",
        "private": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
